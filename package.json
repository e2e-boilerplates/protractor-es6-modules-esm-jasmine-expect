{
  "name": "protractor-es6module-esm-jasmine",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate project. Using ES6 modules, ESM &amp; jasmine",
  "scripts": {
    "upadte-webderiver": "npx webdriver-manager update",
    "test": "npx protractor -r esm protractor.conf.js",
    "test-ci": "npx protractor -r esm protractor-ci.conf.js",
    "lint": "npx eslint --ext .js ./ --fix"
  },
  "keywords": [
    "protractor",
    "es6 modules",
    "esm",
    "jasmine",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "eslint": "6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "esm": "3.2.25",
    "husky": "3.0.9",
    "jasmine": "3.5.0",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "protractor": "5.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "puppeteer": "^2.0.0"
  }
}
